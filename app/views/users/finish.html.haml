.finish-wrapper
  = render 'shared/header'
  = render 'shared/breadcrumbs'

  .finish-wrapper__side
  = render "shared/side_bar"

  .finish-wrapper__main
    .finish-wrapper__main__text 出品した商品
    %ul.finish-wrapper__main__list
      %li.finish-wrapper__main__list__ele1
        =link_to "出品する" , exhibit_user_path(current_user) ,class: "finish-wrapper__main__list__ele1__link"
      %li.finish-wrapper__main__list__ele2
        =link_to "取引中" , business_user_path(current_user) ,class: "finish-wrapper__main__list__ele2__link"
      %li.finish-wrapper__main__list__ele3
        =link_to "売却済み" , finish_user_path(current_user) ,class: "finish-wrapper__main__list__ele3__link"
    .finish-wrapper__main__index
          
  = render 'shared/footer'